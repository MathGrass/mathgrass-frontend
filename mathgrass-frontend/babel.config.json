  "plugins": [["@babel/plugin-proposal-decorators", { "legacy": true}], "@babel/plugin-proposal-class-properties", "@babel/plugin-syntax-jsx"],
  "env": {
    "test": {
      "plugins": [
        [
          "babel-plugin-transform-import-to-require",
